stener);\r\n\r\n        return () => {\r\n            window.removeEventListener('resize', sizeListener);\r\n        }\r\n    }, []);\r\n\r\n    return (\r\n        <div className='content'>\r\n            <Header white={transparentHeader} reducedLogo={mobile} />\r\n            <Outdoor />\r\n            <main className='default-main'>\r\n                <BookInformation />\r\n                <Switch>\r\n                    {routes.map((prop, key) => {\r\n                        if (prop.redirect)\r\n                            return <Redirect from={prop.path} to={prop.to} key={key} />\r\n                        else return (\r\n                            <Route\r\n                                path={prop.path}\r\n                                key={key}\r\n                                exact={true}\r\n                                render={(props) => <prop.component  {...props} />}\r\n                            />\r\n                        )\r\n                    })}\r\n                </Switch>\r\n            </main>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Default)","C:\\Users\\55519\\Documents\\south-system-books\\src\\components\\Header\\index.js",["159"],"import React from 'react'\r\nimport { NavLink } from 'react-router-dom'\r\nimport routes from '../../routes'\r\nimport SearchInput from '../SearchInput'\r\nimport './style.css'\r\n\r\nexport default ({ white, reducedLogo }) => {\r\n    return (\r\n        <header className={white ? 'white' : ''}>\r\n            <div className='header-logo'>\r\n                <NavLink to='/'>\r\n                    {reducedLogo ?\r\n                        <img src='https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQlnekSnuBNoPRRYQVO3AtWtk1D_zPsQWICAw&usqp=CAU' alt='Logo' />\r\n                        : <img src='https://www.pucrs.br/wp-content/themes/pucrs-responsivo/hotsites/tecnopuc-experience/img/logo-southsystem.png' alt='Logo' />}\r\n              